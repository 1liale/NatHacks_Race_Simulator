readEEG <- function(file, raw = FALSE) {
data <- read_csv(file) %>%
rename("Time" = "Secondary Timestamp (Base)",
"Offset" = "Secondary Timestamp (Remainder)")
if (raw) {
return(data)
}
data %>%
rename("Time" = "Secondary Timestamp (Base)",
"Offset" = "Secondary Timestamp (Remainder)") %>%
mutate("timestamps" = NULL,
Time = Time + Offset,
Offset = NULL)
}
