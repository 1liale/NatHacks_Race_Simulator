plotEEG <- function(file, electrode) {
data <- read_csv(file) %>%
rename("Time" = "Secondary Timestamp (Base)",
"Offset" = "Secondary Timestamp (Remainder)") %>%
mutate("timestamps" = NULL,
Time = Time + Offset,
Offset = NULL)
ggplot(data, aes(x = Time, y = eval(parse(text = electrode)))) +
geom_line()
}
